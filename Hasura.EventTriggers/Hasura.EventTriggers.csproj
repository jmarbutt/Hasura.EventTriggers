<Project Sdk="Microsoft.NET.Sdk">

    <PropertyGroup>
        <TargetFramework>net6.0</TargetFramework>
        <ImplicitUsings>enable</ImplicitUsings>
        <Nullable>enable</Nullable>
        <Title>Hasura.EventTriggers</Title>
        <Authors>Jonathan Marbutt</Authors>
        <PackageProjectUrl>https://github.com/jmarbutt/Hasura.EventTriggers</PackageProjectUrl>
        <PackageLicenseUrl></PackageLicenseUrl>
        <PackageIcon>hasura-icon-primary.png</PackageIcon>
        <RepositoryUrl>https://github.com/jmarbutt/Hasura.EventTriggers</RepositoryUrl>
        <RepositoryType>git</RepositoryType>
        <PackageTags>Hasura</PackageTags>
        <PackageVersion>1.0.3</PackageVersion>
        <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
        <PackageReleaseNotes>This is an ALPHA release </PackageReleaseNotes>
    </PropertyGroup>

    <ItemGroup>
      <None Update="hasura-icon-primary.png">
        <Pack>True</Pack>
        <PackagePath></PackagePath>
      </None>
    </ItemGroup>

    <ItemGroup>
      <PackageReference Include="RestSharp" Version="108.0.1" />
    </ItemGroup>

</Project>
